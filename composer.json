{
    "name": "filakitphp/installer",
    "description": "Filakit application installer.",
    "keywords": [
        "filakit"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferson Simão Gonçalves",
            "email": "gerson.simao.92@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0",
        "symfony/polyfill-mbstring": "^1.31"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/filakit"
    ],
    "autoload": {
        "psr-4": {
            "Filakit\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filakit\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
